{
  "name": "email-constructor",
  "version": "0.0.2",
  "description": "Open-source HTML E-mail constructor",
  "main": "index.html",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html --public-url ./",
    "add-domain": "mkdir -p dist && echo \"email.lepekhin.studio\" > dist/CNAME",
    "predeploy": "npm run add-domain && npm run build",
    "deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lepekhin/email-constructor.git"
  },
  "keywords": [
    "html",
    "email"
  ],
  "author": "https://lepekhin.studio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lepekhin/email-constructor/issues"
  },
  "homepage": "https://email.lepekhin.studio",
  "dependencies": {
    "@svgr/parcel-plugin-svgr": "^5.5.0",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.2.3",
    "parcel-bundler": "^1.12.5",
    "typescript": "^4.4.4"
  }
}
